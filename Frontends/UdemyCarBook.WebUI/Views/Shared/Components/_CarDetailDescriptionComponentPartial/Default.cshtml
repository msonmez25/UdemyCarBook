@model List<ResultCarDescriptionByCarIdDto>


 @if(@Model.Count > 0)
{

    @foreach(var item in Model)
    {
        <div class="tab-pane fade" id="pills-manufacturer" role="tabpanel" aria-labelledby="pills-manufacturer-tab">
            <p>@item.Details</p>
        </div>
        
    }

}
else
{
    <div class="tab-pane fade" id="pills-manufacturer" role="tabpanel" aria-labelledby="pills-manufacturer-tab">
        <p>İlgili aracın henüz bir açıklaması bulunumamaktadır.</p>
    </div>  
}








                          
